{
  "name": "vscode-flow-ide",
  "displayName": "vscode-flow-ide",
  "description": "fork of vscode-flow-ide that uses flow-bin installed in each directory",
  "version": "1.2.2",
  "publisher": "jedwards1211",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "flow.coverage",
        "title": "Flow-IDE: Toggle Flowtype coverage"
      },
      {
        "command": "flow.path",
        "title": "Flow-IDE: Show path to Flow"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Flow-IDE Configurations",
      "properties": {
        "flowide.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Is flow enabled"
        },
        "flowide.useCodeSnippetsOnFunctionSuggest": {
          "type": "boolean",
          "default": true,
          "description": "Whether should autocomplete params name"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/find-root": "^1.1.1",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/resolve": "^1.14.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "cross-spawn": "^4.0.2",
    "find-root": "^1.1.0",
    "js-beautify": "^1.6.4",
    "resolve": "^1.15.1"
  },
  "icon": "flow-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcazaciuc/vscode-flow-ide"
  },
  "bugs": {
    "url": "https://github.com/gcazaciuc/vscode-flow-ide/issues"
  }
}
